# Application
APP_NAME=DocHandler
NODE_ENV=development

# Server Configuration
VITE_PORT=3000
VITE_API_PORT=8080
VITE_HOST=localhost

# Ollama Configuration
VITE_OLLAMA_HOST=http://localhost:11434
VITE_OLLAMA_MODELS=llama2-7b-chat,mistral-7b

# Vector Database
VITE_VECTORS_PATH=./data/vectors
VITE_DB_PATH=./data/db

# External Services (for future use)
VITE_OPENAI_API_KEY=
VITE_ANTHROPIC_API_KEY=
VITE_NOMIC_API_KEY=

# Security
VITE_JWT_SECRET=dev_secret
VITE_COOKIE_SECRET=dev_cookie_secret

# Monitoring
VITE_LOGGING_LEVEL=debug

# Document Processing
VITE_MAX_FILE_SIZE=50mb
VITE_ALLOWED_FILE_TYPES=.txt,.pdf,.doc,.docx,.md
VITE_MAX_CONCURRENT_INDEXING=3

# Embedding Configuration
VITE_DEFAULT_EMBEDDING_MODEL=nomic-embed-text-v1
VITE_EMBEDDING_BATCH_SIZE=32
VITE_VECTOR_DIMENSION=768